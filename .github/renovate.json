{
  "baseBranches": [
    "master"
  ],
  "supportPolicy": ["all"],
  "travis": { "enabled": true },
  "packageRules": [
    {
      "packagePatterns": [
        "^restify*"
      ],
      "groupName": "restify"
    },
    {
      "depTypeList": ["devDependencies"],
      "automerge": true
    }
  ],
  "vulnerabilityAlerts": {
    "labels": ["security"],
    "assignees": ["@frbuceta"]
  }
}
